while (y>0) {
    y-=8;
    // c=crashTo(クラス名) は，そのクラスのオブジェクトのどれかと
    // 衝突しているかを判定します．
    c=crashTo(Chaser);
    // どのCheseオブジェクトとも衝突していない 
    //      -> c==undefined
    // 1つ以上のChaseオブジェクトと衝突している 
    //      -> c==衝突しているChaseオブジェクトのうちどれか1つ
    if (c) {
        c.die();
        die();
        $score+=10;
    }
    update();
}
// [!=Tonyu1] 処理の最後で消滅させたいときはdie(); を書きます．
die();

