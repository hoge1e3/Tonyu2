extends PlainChar;

\new(x,y,p,f) {
    super(x,y,p);
    this.f=f;
    if (!this.x) this.x=0;
    if (!this.y) this.y=0;
    if (!this.p) this.p=0;
    if (typeof scaleX!="number") scaleX=1;
}
\draw(c) {
    if (_isInvisible) return;
    onDraw();
    prepareImage();
    drawSprite(x,y,p,f,zOrder);
    /*if (f) {
        if (!scaleY) scaleY=scaleX;
        scaleX*=-1;
    }
    super.draw(c);
    if (f) scaleX*=-1;*/
}