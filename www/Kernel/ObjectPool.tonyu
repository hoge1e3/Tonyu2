extends null;
native Object;

\pool(key,o) {
	var list=poolList(key);
	list.push(o);
}
\withdraw(key) {
	var list=poolList(key);
	return list.shift();
}
\poolList(key) {
	lists=lists||{};
	return lists[key]=lists[key]||[];
}
\setup(klass) {
	var k=klass.meta.fullName;
	lists=lists||{};
	lists[k]=[];
}
