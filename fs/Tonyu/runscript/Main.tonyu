while(!$gov) {
    if (tick>32) {
        new Ball{p:$pat_balls+rnd(25),x:rnd($screenWidth),y:0};
        tick-=32;
    }
    if (all(Ball).max("y")<$screenHeight-200) {
        $spd=5;
    } else {
        $spd=1;
    }
    tick+=$spd;
    if (all(X).size()>=3) $gov=true;
    update();
}
