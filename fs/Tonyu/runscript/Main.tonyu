tick=32;
new MayGet{x:200, y:250, t:-1};
new MayGet{x:100, y:300, t:0};
new MayGet{x:100, y:350, t:1};
new MayGet{x:320, y:300, t:2};
new MayGet{x:320, y:350, t:3};
