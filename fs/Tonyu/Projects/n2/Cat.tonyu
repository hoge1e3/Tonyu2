p=$pat_neko+0;
x=$screenWidth/2;
y=$screenHeight/2;
on("crashTo",Fish,crashFunc);

while(true){
    if (getkey("Right")) {
        x+=5;
    }
    if (getkey("Left")) {
        x-=5;
    }
    if (getkey("Down")) {
        y+=5;
    }
    if (getkey("Up")) {
        y-=5;
    }
    update();
}
\crashFunc(f){
    f.goRight();
}