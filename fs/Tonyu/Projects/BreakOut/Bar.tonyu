extends BodyActor;

while (true) {
    if (getkey("left")) {
        applyForce(-40,0);        
    }
    if (getkey("right")) {
        applyForce(40,0);
    }
    if (getkey("up")) {
        applyImpulse(0,-2);
    }
    if (getkey("down")) {
        applyImpulse(0,2);
    }
    update();
}
