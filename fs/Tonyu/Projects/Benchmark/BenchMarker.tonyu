extends TObject;
native Date;
native console;
marks={};
\start(id) {
    if (!marks[id]) marks[id]={count:0,times:0};
    marks[id].start=new Date().getTime();
}
\end(id) {
    var m=marks[id];
    m.count++;
    m.times+=new Date().getTime()-m.start;
}
\reset() {
    oldMarks=marks;
    marks={};
}
\report() {
    console.log(marks);
    /*    for (var id,m in marks) {
        print(id+" - cnt="+m.count+"  times="+m.times);
    }*/
}