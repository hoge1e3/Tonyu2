extends Actor;
//てすと！
scaleX=2;
scaleY=3;
//rotate=45;
if (!vx) vx=0;
if (!vy) vy=0;
while (true) {
    x+=vx;
    y+=vy;
    c=crashTo(AcTest);
    if (c) {
        if (x>c.x) {
            //vx=5; 
            vx=0;
            c.vx=0;
            //c.vx=-5;
        } else {
            //vx=-5;
            vx=0;
            c.vx=0;
            //c.vx=5;
        }
        vy=0;
        c.vy=0;
        print("Hit!!");
    }
    update();
}
isDead();

