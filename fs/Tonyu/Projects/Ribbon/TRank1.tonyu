$gcont=0;
gcon=0;
p=-1;
x=400;
y=400;
while(1) {
    // ランクを計算する
    rank=0;
    px=$VTR.x;
    py=$VTR.y;
    while (dist($VTR.x-px,$VTR.y-py)<10) {
        // マウスを動かしていないとランクが上がる
        px=$VTR.x;
        py=$VTR.y;
        rank=rank+2;
        update();
    }
    update();
    $gcont=0;
}