extends TRank0;
// ※このクラスは、TRank0クラスを継承しています。「クラス」ウィンドウでTRank0も参照してください

\new(x,y,r,gc) {
    super(x,y,$pat_star+2);
    rank=r;
    gcon=gc;
}
p=-1;
i=60;
// ボール破壊時に出現する"Bad" "Good" "Great"の表示
while (i>0) {
    i=i-1;
    if (i % 2<1) {
        // putRankはTRank0で定義されている
        putRank();
    }
    update();
}
