function putRank(){
    var s,sc;
    // ランクにより Bad,Good Greatを表示する
    if (rank<10) {
        drawText(x,y,"Bad",color(0,255,0));
    } else if (rank<15) {
        drawText(x,y,"Good",color(255,255,0));
    } else {
        s="Great ";
        if (gcon>1) s=s+gcon;
        drawText(x,y,s,color(255,155,0));
    }
}