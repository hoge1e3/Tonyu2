x=80;
y=100;
while(true) {
    k=getkey("right");
    if (k>0) {
        x+=10;
    }
    k=getkey("left");
    if (k>0) {
        x-=10;
    }
    k=getkey("down");
    if (k>0) {
        y+=10;
    }
    k=getkey("up");
    if (k>0) {
        y-=10;
    }
    a=crashTo(Apple);
    if (a) {
        a.hide();
    }
    go(x,y);sleep();
}