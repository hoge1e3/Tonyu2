extends TObject;
native T;

tbl={};
\set(num, synth) {
    tbl[num]=synth;
}
\get(num) {
    return tbl[num];
}
\stop() {
    for (var k,v in tbl) {
        v.pause();
        v.stop();
    }
}

if (typeof T!=="undefined") {
    env=T("adsr", {a:0,d:200,s:0.5,r:10});
    synth=T("OscGen") {wave:"pulse", env, mul:0.25};
    set(0,synth);    
}
